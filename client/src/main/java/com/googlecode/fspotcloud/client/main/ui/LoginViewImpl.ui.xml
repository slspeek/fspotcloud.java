<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:my="urn:import:com.googlecode.fspotcloud.client.main.ui">
	<ui:with type="com.googlecode.fspotcloud.client.main.ui.Resources" field="res"></ui:with>
	<g:LayoutPanel ui:field="mainPanel" styleName="{res.style.tagView}">
		<g:layer top="0%" height="20%" right="0%" width="100%">
			      <g:HTML><center><h1>Login</h1></center></g:HTML>
		</g:layer>
		<g:layer top="25%" height="5%" left="30%" width="40%">
             <g:TextBox ui:field="userNameTextBox"></g:TextBox>
		</g:layer>
        <g:layer top="50%" height="5%" left="30%" width="40%">
             <g:PasswordTextBox ui:field="passwordTextBox"></g:PasswordTextBox>
        </g:layer>
	</g:LayoutPanel>
</ui:UiBinder> 