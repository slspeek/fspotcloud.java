<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c='urn:import:com.google.gwt.user.cellview.client'>
	<ui:with type="com.googlecode.fspotcloud.client.main.ui.Resources" field="res"></ui:with>
	<g:LayoutPanel >
		<g:layer top="0%" height="10%" left="0%" width="100%">
		  <g:HTMLPanel><center><h1>Granting to tag</h1></center></g:HTMLPanel>
		</g:layer>
        <g:layer top="0%" height="10%" right="0%" width="100%">
            <g:Label ui:field="tagName"></g:Label>
        </g:layer>
		<g:layer top="40%" height="50%" left="20%" width="30%">
            <c:CellTable ui:field="approved"></c:CellTable>
		</g:layer>
        <g:layer top="40%" height="50%" left="60%" width="30%">
            <c:CellTable ui:field="other"></c:CellTable>
        </g:layer>
        <g:layer top="80%" height="40px" left="30%" width="10%">
            <g:PushButton ui:field="approveButton">Approve</g:PushButton>
        </g:layer>
        <g:layer top="80%" height="40px" left="45%" width="10%">
            <g:PushButton ui:field="removeButton">Remove</g:PushButton>
        </g:layer>

	</g:LayoutPanel>
</ui:UiBinder> 