<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:c='urn:import:com.google.gwt.user.cellview.client'>
    <ui:with type="com.googlecode.fspotcloud.client.main.ui.Resources" field="res"></ui:with>
    <g:LayoutPanel>
        <g:layer top="0%" height="10%" right="0%" width="100%">
            <g:HTML>
                <center>
                    <h1>Usergroups: manage users</h1>
                </center>
            </g:HTML>
        </g:layer>
        <g:layer top="10%" height="40px" left="30%" width="20%">
            <g:Label ui:field="userGroupName"></g:Label>
        </g:layer>
        <g:layer top="0%" height="70%" left="30%" width="60%">
            <c:CellTable ui:field="table"></c:CellTable>
        </g:layer>
        <g:layer top="80%" height="40px" left="30%" width="10%">
            <g:TextBox ui:field="emailTextBox"></g:TextBox>
        </g:layer>
        <g:layer top="80%" height="40px" left="45%" width="10%">
            <g:PushButton ui:field="newButton">Add user</g:PushButton>
        </g:layer>
        <g:layer top="80%" height="40px" left="60%" width="10%">
            <g:PushButton ui:field="deleteButton">Delete</g:PushButton>
        </g:layer>
    </g:LayoutPanel>
</ui:UiBinder> 