<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<table>
			<theader>
				<tr>
					<td colspan="all">
						<g:Label ui:field="headerLabel">Label details</g:Label>
					</td>
				</tr>
			</theader>
			<tbody>
				<tr>
					<td>
						<g:Label ui:field="tagNameLabel">Name</g:Label>
					</td>
					<td>
						<g:Label ui:field="tagNameValueLabel"></g:Label>
					</td>
				</tr>
				<tr>
					<td>
						<g:Label ui:field="tagDescriptionLabel">Description</g:Label>
					</td>
					<td>
						<g:Label ui:field="tagDescriptionValueLabel"></g:Label>
					</td>
				</tr>
				<tr>
					<td>
						<g:Label ui:field="tagImportIssuedLabel">Import was requested</g:Label>
					</td>
					<td>
						<g:Label ui:field="tagImportIssuedValueLabel"></g:Label>
					</td>
				</tr>
				<tr>
					<td>
						<g:Label ui:field="tagCountLabel">Number of photos in this label</g:Label>
					</td>
					<td>
						<g:Label ui:field="tagCountValueLabel"></g:Label>
					</td>
				</tr>
				<tr>
					<td>
						<g:Label ui:field="tagLoadedCountLabel">Number of photos loaded</g:Label>
					</td>
					<td>
						<g:Label ui:field="tagLoadedCountValueLabel"></g:Label>
					</td>
				</tr>
				<tr>
					<td>
						<g:Label ui:field="importTagsLabel">Request import of this label</g:Label>
					</td>
					<td>
						<g:Button ui:field="importTagButton" >Import label</g:Button>
					</td>
				</tr>
			</tbody>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 