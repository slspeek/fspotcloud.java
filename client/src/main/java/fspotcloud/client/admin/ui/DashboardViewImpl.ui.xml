<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:main="urn:import:fspotcloud.client.main.ui"
	xmlns:admin="urn:import:fspotcloud.client.admin.ui">
	<ui:style>
		
	</ui:style>
	<g:DockLayoutPanel unit='PX'>
		<g:north size="30">
			<g:Label ui:field="titleLabel" text="FSpotCloud Dashboard">
			</g:Label>
		</g:north>
		<g:east size="5">
			<g:HTML>Tree</g:HTML>
		</g:east>
		<g:center>
			<g:SplitLayoutPanel ui:field="horizontalSplitPanel">
				<g:west size="250">
					<main:TreeViewImpl ui:field="treeView"></main:TreeViewImpl>
				</g:west>
				<g:center>
					<g:HTML>
						<table>
							<tbody>
								<tr>
									<td>
										<g:SimplePanel ui:field="tagDetailsViewContainer"></g:SimplePanel>
									</td>
									<td>
										<admin:GlobalActionsViewImpl
											ui:field="globalActionsView"></admin:GlobalActionsViewImpl>
									</td>
								</tr>
							</tbody>
						</table>
					</g:HTML>
				</g:center>
			</g:SplitLayoutPanel>
		</g:center>
		<g:south size='2'>
			<g:HTML>Footer</g:HTML>
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder> 