<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:fspotcloud.client.main.ui" >
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:DockLayoutPanel unit="PX" ui:field="mainPanel">
			<g:north size="80">
				<g:Label ui:field="titleLabel" text="FSpotCloud Java Edition">
				</g:Label>
			</g:north>
			<g:south size="40">
				<g:FlowPanel ui:field="statusPanel">
					<g:Label ui:field="statusLabel"></g:Label>
				</g:FlowPanel>
			</g:south>
			<g:center>
				<g:SplitLayoutPanel>
					<g:west size="200">
					<my:TreePanel></my:TreePanel>
					</g:west>
				</g:SplitLayoutPanel>
			</g:center>
		</g:DockLayoutPanel>
	</g:HTMLPanel>

</ui:UiBinder> 