<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	</ui:style>
	<ui:with type="fspotcloud.client.main.ui.Resources" field="res"></ui:with>
	<g:HorizontalPanel ui:field="mainPanel">
		<g:PushButton ui:field="slowerButton" text="Decrease"></g:PushButton>
		<g:PushButton ui:field="startButton">
		<g:upFace image="{res.playIcon}"> </g:upFace>
		</g:PushButton>
		<g:Label ui:field="intervalLabel"></g:Label>
		<g:PushButton ui:field="stopButton" text="Stop"></g:PushButton>
		<g:PushButton ui:field="fasterButton" text="Increase"></g:PushButton>
	</g:HorizontalPanel>
</ui:UiBinder> 