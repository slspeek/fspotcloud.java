<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:my="urn:import:fspotcloud.client.main.ui">
	<ui:style>
		table {
			margin: auto;
		}
	</ui:style>
	<g:DockLayoutPanel ui:field="mainPanel">
		<g:center>
			<my:ImageRasterViewImpl></my:ImageRasterViewImpl>
		</g:center>
		<g:south size="100">
			<g:HTMLPanel ui:field="buttonPanel">
				<center>
					<g:VerticalPanel>
						<g:HorizontalPanel>
							<my:PagerViewImpl></my:PagerViewImpl>
							<my:SlideshowViewImpl></my:SlideshowViewImpl>
						</g:HorizontalPanel>
						<my:ButtonPanelViewImpl></my:ButtonPanelViewImpl>
					</g:VerticalPanel>
				</center>
			</g:HTMLPanel>
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder> 
