<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		table {
			text-align: center;
		}
		.raster {
			background: aqua;
			width: 100%;
			height: 100%;
			margin: auto;
			}
	</ui:style>
	<g:SimplePanel styleName="{style.raster}" ui:field="simplePanel"></g:SimplePanel>
</ui:UiBinder> 