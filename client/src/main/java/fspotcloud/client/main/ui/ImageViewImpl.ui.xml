<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:fit="urn:import:com.reveregroup.gwt.imagepreloader">
	<ui:with type="fspotcloud.client.main.ui.Resources" field="res"></ui:with>
	<g:HTMLPanel>
		<div class="{res.style.lighterBlock}">
			<fit:FitImage ui:field="image"></fit:FitImage>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 