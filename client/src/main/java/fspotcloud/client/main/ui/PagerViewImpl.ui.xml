<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		table {
			width: 100%;
			}
	</ui:style>
	<g:HorizontalPanel ui:field="mainPanel">
		<g:PushButton ui:field="firstButton" text="First"></g:PushButton>
		<g:PushButton ui:field="prevButton" text="Previous"></g:PushButton>
		<g:PushButton ui:field="nextButton" text="Next"></g:PushButton>
		<g:PushButton ui:field="lastButton" text="Last"></g:PushButton>
	</g:HorizontalPanel>
</ui:UiBinder> 