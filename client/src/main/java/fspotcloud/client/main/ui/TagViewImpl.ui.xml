<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v="urn:import:com.google.gwt.user.cellview.client"
	xmlns:my="urn:import:fspotcloud.client.main.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel ui:field="mainPanel">
		<g:Label ui:field="titleLabel" text="FSpotCloud Java Edition">
		</g:Label>
		<g:SplitLayoutPanel height="700px" ui:field="horizontalSplitPanel">
			<g:west size="380"> 
				<my:TreeViewImpl ui:field="treeView"></my:TreeViewImpl>
			</g:west>
			<g:center>
				<g:SimplePanel  height="660px" ui:field="imageViewPanel"></g:SimplePanel>
			</g:center>
		</g:SplitLayoutPanel>
		<g:FlowPanel ui:field="statusPanel">
			<g:Label ui:field="statusLabel" text="Press h for help"></g:Label>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 