<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:my="urn:import:fspotcloud.client.main.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel ui:field="mainPanel">
		<g:Label ui:field="titleLabel" text="FSpotCloud Java Edition">
		</g:Label>
		<g:HorizontalSplitPanel>
			<g:Tree ui:field="tagTree"></g:Tree>
			<g:Image ui:field="mainImage"
				url="http://fspotcloud.appspot.com/image/10471/1"></g:Image>
		</g:HorizontalSplitPanel>
		<g:FlowPanel ui:field="statusPanel">
			<g:Label ui:field="statusLabel" text="Ready"></g:Label>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 