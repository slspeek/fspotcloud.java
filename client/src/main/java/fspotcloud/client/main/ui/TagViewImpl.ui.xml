<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:my="urn:import:fspotcloud.client.main.ui">
	<ui:with type="fspotcloud.client.main.ui.Resources" field="res"></ui:with>
	<g:LayoutPanel ui:field="mainPanel" styleName="{res.style.tagView}">


		<g:layer top="0%" height="90%" right="0%" width="78%">
			
			<my:ImageRasterViewImpl ui:field="imageRasterView"></my:ImageRasterViewImpl>
			
		</g:layer>

		<g:layer top="0%" height="90%" left="0%" width="22%">
			<g:FocusPanel ui:field="treeFocusPanel">
				<my:TreeViewImpl styleName="{res.style.treeBlock}"></my:TreeViewImpl>
			</g:FocusPanel>
		</g:layer>
		<g:layer bottom="0%" height="10%">
			<g:FocusPanel ui:field="buttonFocusPanel">
				<my:ButtonPanelViewImpl></my:ButtonPanelViewImpl>
			</g:FocusPanel>
		</g:layer>
		<g:layer bottom="0%" height="0%">
			<g:FocusPanel ui:field="horizontalFocusPanel"></g:FocusPanel>
		</g:layer>
		<g:layer left="0%" width="0%">
			<g:FocusPanel ui:field="verticalFocusPanel"></g:FocusPanel>
		</g:layer>
	</g:LayoutPanel>
</ui:UiBinder> 