<configurations>
  <configuration name="Photo Counter Mapper">
    <property>
      <name>mapreduce.map.class</name>
      <value>fspotcloud.server.mapreduce.PhotoCounterMapper</value>
    </property>
    <property>
      <name>mapreduce.inputformat.class</name>
      <value>com.google.appengine.tools.mapreduce.DatastoreInputFormat</value>
    </property>
    <property>
      <name human="Entity Kind to Map Over">mapreduce.mapper.inputformat.datastoreinputformat.entitykind</name>
      <value template="optional">PhotoDO</value>
    </property>
  </configuration>
</configurations>
